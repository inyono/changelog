{
  "name": "@inyono/changelog",
  "version": "0.4.0",
  "homepage": "https://github.com/inyono/changelog",
  "bugs": {
    "url": "https://github.com/inyono/changelog/issues"
  },
  "repository": "inyono/changelog",
  "license": "MIT",
  "author": "Jonas Keinholz <jonas@keinholz.dev>",
  "type": "module",
  "exports": "./index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "_prettier": "prettier .",
    "build": "rimraf dist && tsup src/index.ts --dts --format esm,cjs",
    "changelog": "node --experimental-specifier-resolution=node --loader ts-node/esm scripts/changelog",
    "format": "yarn _prettier --write",
    "lint": "npm-run-all --parallel lint:prettier lint:tsc",
    "lint:prettier": "yarn _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "test": "node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@types/mdast": "^3.0.10",
    "mdast": "^3.0.0",
    "prettier": "^2.5.1",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-packagejson": "^2.2.16",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "tsup": "^5.12.0",
    "typescript": "^4.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.2.0"
}
